{
  "name": "bond-mx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "cross-env VITE_PROJ=bond vite",
    "dev:ota": "cross-env VITE_PROJ=ota vite",
    "build": "cross-env VITE_PROJ=bond tsc -b && vite build",
    "build:ota": "cross-env VITE_PROJ=ota tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "prepare": "husky",
    "deploy": "gh-pages -d dist --no-history",
    "postinstall": "node ./postinstall.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@lexical/react": "^0.17.1",
    "@react-pdf/renderer": "^3.4.4",
    "@types/antd": "^1.0.0",
    "@untitled-ui/icons-react": "^0.1.3",
    "ahooks": "^3.8.1",
    "antd": "^5.20.6",
    "antd-img-crop": "^4.23.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.6",
    "framer-motion": "^11.5.4",
    "jwt-decode": "^4.0.0",
    "lexical": "^0.17.1",
    "lodash": "^4.17.21",
    "mobx": "^6.13.2",
    "mobx-react": "^9.1.1",
    "normalize.css": "^8.0.1",
    "overlayscrollbars": "^2.10.0",
    "overlayscrollbars-react": "^0.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-international-phone": "^4.3.0",
    "react-markdown": "^9.0.1",
    "react-pdf": "^9.1.0",
    "react-router-dom": "^6.26.2",
    "react-scroll-up": "^1.4.0",
    "react-share": "^5.1.1",
    "react-to-print": "^3.0.3",
    "styled-components": "^6.1.13",
    "swiper": "^11.1.14",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
